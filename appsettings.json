{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.RollingFile", "Serilog.Sinks.Async" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "logs/dispatcher-{Date}.log"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "IdentityServer"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:5205",
  "Hangfire": {
    "WorkerCount": 15,
    "AutoUpdate_CronExpression": "0 0/5 * * * ? ",
    "DeleteInvalidPyhsicalTable_CronExpression": "0 0 2 * * ? "
  }
}
